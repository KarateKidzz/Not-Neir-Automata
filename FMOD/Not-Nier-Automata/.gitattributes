# FMOD Supported Audio Files
*.aiff filter=lfs diff=lfs merge=lfs -text
*.asf filter=lfs diff=lfs merge=lfs -text
*.dls filter=lfs diff=lfs merge=lfs -text
*.flac filter=lfs diff=lfs merge=lfs -text
*.it filter=lfs diff=lfs merge=lfs -text
*.midi filter=lfs diff=lfs merge=lfs -text
*.mod filter=lfs diff=lfs merge=lfs -text
*.mpeg filter=lfs diff=lfs merge=lfs -text
*.oggvorbis filter=lfs diff=lfs merge=lfs -text
*.playlist filter=lfs diff=lfs merge=lfs -text
*.raw filter=lfs diff=lfs merge=lfs -text
*.s3m filter=lfs diff=lfs merge=lfs -text
*.user filter=lfs diff=lfs merge=lfs -text
*.wav filter=lfs diff=lfs merge=lfs -text
*.xm filter=lfs diff=lfs merge=lfs -text
*.xma filter=lfs diff=lfs merge=lfs -text
*.audioqueue filter=lfs diff=lfs merge=lfs -text
*.vorbis filter=lfs diff=lfs merge=lfs -text
*.fadpcm filter=lfs diff=lfs merge=lfs -text
*.opus filter=lfs diff=lfs merge=lfs -text
*.max filter=lfs diff=lfs merge=lfs -text

# FMOD Build Files
*.bank filter=lfs diff=lfs merge=lfs -text

# Other Audio Files
*.mp3 filter=lfs diff=lfs merge=lfs -text